{
  "name": "checkmate",
  "version": "0.0.1",
  "manifest_version": 2,
  "platform_app": true,
  "background": {
    "scripts": ["js/background.js"]
  },
  "permissions": [
    "experimental",
    "windows",
    "*://blog.chromium.org/feeds/*"
  ],
  "content_security_policy": "script-src 'self' https://blog.chromium.org; object-src 'self'",
  "intents": {
    "http://webintents.org/view": [
      { 
        "type": ["application/rss+xml", "application/atom+xml"],
        "href": "/index.html",
        "title": "checkmate",
        "disposition" : "inline"
      }
    ]
  }
}
