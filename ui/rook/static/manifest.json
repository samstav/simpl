{
  "name": "checkmate",
  "version": "0.0.1",
  "manifest_version": 2,
  "background": {
    "scripts": ["/js/background.js"]
  },
  "permissions": [
    "experimental",
    "windows",
    "*://blog.chromium.org/feeds/*",
    "http://localhost/",
    "https://checkmate.rackspace.com/*",
    "https://identity.api.rackspacecloud.com/v2.0/tokens",
    "https://*.identity.api.rackspacecloud.com/v2.0/tokens"
  ],
  "app": {
    "launch": {
      "local_path": "/index.html"
    }
  },
  "content_security_policy": "script-src 'self' http://localhost 'unsafe-eval'; object-src 'self'  http://localhost"
}
